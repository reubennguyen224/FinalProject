<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppBarTheme" >
        <item name="backgroundColor">#06AB8D</item>
        <item name="colorPrimary">#ffffff</item>
        <item name="colorPrimaryVariant">#fffff2</item>
        <item name="android:textColor">#ffffff</item>
        <item name="fontFamily">@font/poppins</item>
        <item name="lineHeight">21sp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:letterSpacing">0.01</item>

    </style>
    <style name="rounded_corner">
        <item name="cornerSize"> 10dp </item>
        <item name="cornerFamily">rounded</item>
    </style>
    <style name="bound_corner">
        <item name="cornerSizeBottomLeft">15dp</item>
        <item name="cornerSizeBottomRight">15dp</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
    </style>
    <style name="RattingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">#e9edef</item>
        <item name="colorControlActivated">#FFC061</item>
    </style>
</resources>
